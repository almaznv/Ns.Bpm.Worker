        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
    </startup>
    <appSettings>
      <add key="workerName" value="NsWorker_Localhost_1" />
      <add key="rabbitMqHost" value="localhost:5673" />
